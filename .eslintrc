{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true,
            "classes": true,
            "arrowFunctions": true
        },
    },
    "rules": {
        "quotes": [
            0
        ],
        "linebreak-style": [
            0
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": [
            1,
            {
                "varsIgnorePattern": "^_"
            }
        ],
        "no-undef": [
            1
        ],
        "no-console": [
            1
        ],
        "no-var": [
            2
        ]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}